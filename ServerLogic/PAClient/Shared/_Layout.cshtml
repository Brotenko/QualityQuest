    <div id="site-content" class="container content-container">
        <main>
            @RenderBody()
        </main>
    </div>
    <footer id="footer" class="border-top footer text-muted">
        <div class="container">
            2021 - <a target="_blank" rel="noopener noreferrer" href="https://github.com/Brotenko/QualityQuest" style="color: inherit">Quality Quest</a> - <a runat="server" asp-page="/Privacy">Privacy</a>
            <span class="footer-logo-container">
                Made in cooperation with: &nbsp;
                <a target="_blank" rel="noopener noreferrer" href="https://www.newtec.de/">
                    <img src="~/newtec_logo.png" alt="NewTec logo" href="https://www.newtec.de/" class="footer-logo" />
                </a>
            </span>
        </div>
    </footer>

